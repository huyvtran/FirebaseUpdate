    "react-native-button": "2.2.0",
    "react-native-code-push": "^6.3.0",
    "react-native-confirmation-code-input": "https://github.com/boythan/react-native-confirmation-code-input.git",
    "react-native-datepicker": "1.7.2",
    "react-native-device-info": "2.1.3",
    "react-native-dropdownalert": "3.4.0",
    "react-native-elements": "1.2.7",
    "react-native-fingerprint-scanner": "3.0.2",
    "react-native-freshchat-sdk": "2.0.0",
    "react-native-geolocation-service": "3.0.0",
    "react-native-gps-state": "2.0.3",
    "react-native-htmlview": "0.13.0",
    "react-native-i18n": "2.0.15",
    "react-native-image-picker": "^2.3.1",
    "react-native-interactable": "^2.0.1",
    "react-native-maps": "0.26.1",
    "react-native-material-design-styles": "0.2.7",
    "react-native-message-bar": "2.0.4",
    "react-native-modal": "4.1.1",
    "react-native-modalbox": "1.4.2",
    "react-native-open-maps": "0.1.3",
    "react-native-orientation": "3.1.3",
    "react-native-permissions": "git+https://github.com/boythan/react-native-permissions.git",
    "react-native-root-toast": "3.1.2",
    "react-native-router-flux": "4.0.0-beta.31",
    "react-native-scrollable-tab-view": "0.10.0",
    "react-native-signature-capture": "0.4.10",
    "react-native-size-matters": "0.1.6",
    "react-native-snap-carousel": "3.7.5",
    "react-native-splash-screen": "3.2.0",
    "react-native-svg": "9.8.4",
    "react-native-system-setting": "1.5.3",
    "react-native-tab-view": "1.3.0",
    "react-native-vector-icons": "6.6.0",
    "react-native-version-check": "3.1.0",
    "react-native-wheel-pick": "git+https://github.com/boythan/react-native-wheel-pick.git",
    "react-redux": "5.0.5",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-persist": "6.0.0",
    "redux-saga": "0.15.6",
    "redux-thunk": "2.2.0",
    "rn-fetch-blob": "0.10.16"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.13.1",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.162",
    "@types/react": "^16.9.56",
    "@types/react-test-renderer": "^16.9.3",
    "babel-plugin-transform-remove-console": "6.8.5",
    "detox": "16.0.1",

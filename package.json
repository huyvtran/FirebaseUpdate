{
  "name": "FirebaseUpdate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "start": "npx react-native start",
    "test": "jest",
    "lint": "eslint .",
    "installWin": "del package-lock.json yarn.lock && npm install && yarn",
    "reinstallWin": "del node_modules package-lock.json yarn.lock && npm install && yarn",
    "migrateAndroidX": "npx jetify",
    "adb-reverse": "adb reverse tcp:8081 tcp:8081",
    "androidSdk": "echo \"sdk.dir=$ANDROID_HOME\" > android/local.properties",
    "ia": "adb devices | awk 'NR>1 && $1 ~ /[:]+$/ { system(\"adb -s \" $1 \" install -r android/app/build/outputs/apk/app-release.apk\") }'",
    "keyiOS": "code-push deployment ls $npm_package_name-ios -k",
    "keyAndroid": "code-push deployment ls $npm_package_name-android -k",
    "releaseiOS": "code-push release-react $npm_package_name-ios ios --description $npm_package_push_description",
    "releaseAndroid": "code-push release-react $npm_package_name-android android --description $npm_package_push_description",
    "releaseForceiOS": "code-push release-react $npm_package_name-ios ios -m --description $npm_package_push_description",
    "releaseForceAndroid": "code-push release-react $npm_package_name-android android -m --description $npm_package_push_description",
    "promoteiOS": "code-push promote $npm_package_name-ios Staging Production",
    "promoteAndroid": "code-push promote $npm_package_name-android Staging Production",
    "rollbackStagingiOS": "code-push rollback $npm_package_name-ios Staging",
    "rollbackStagingAndroid": "code-push rollback $npm_package_name-android Staging",
    "rollbackProiOS": "code-push rollback $npm_package_name-ios Production",
    "rollbackProAndroid": "code-push rollback $npm_package_name-android Production",
    "statisticStagingiOS": "code-push deployment history $npm_package_name-ios Staging",
    "statisticStagingAndroid": "code-push deployment history $npm_package_name-android Staging",
    "statisticProiOS": "code-push deployment history $npm_package_name-ios Production",
    "statisticProAndroid": "code-push deployment history $npm_package_name-android Production",
    "bundleAbivin": "cd android && ./gradlew :app:bundleAbivinRelease && cd ..",
    "bundleSnp": "cd android && ./gradlew :app:bundleAbivinSnpRelease && cd ..",
    "installMac": "rm -rf node_modules package-lock.json yarn.lock && npm install && yarn",
    "uiTest": "detox test ./test/uiautomation/",
    "nodeStartTest": "RN_SRC_EXT=e2e.js react-native start",
    "android:beta": "fastlane android beta",
    "ios:beta": "fastlane ios beta",
    "abivinRelease": "react-native run-android --variant=abivinRelease",
    "androidDebugAbivin": "react-native run-android --variant=abivinDebug",
    "abivinDemoDebug": "react-native run-android --variant=abivinDemoDebug",
    "abivinDemoRelease": "cd android && ./gradlew clean && ./gradlew assembleAbivinDemoRelease && cd ..",
    "androidDebugSnp": "react-native run-android --variant=abivinSnpDebug",
    "androidDemoSnp": "cd android && ./gradlew clean && ./gradlew assembleAbivinSnpDemoRelease && cd ..",
    "abAbivin": "cd android && ./gradlew clean && ./gradlew assembleAbivinRelease && cd ..",
    "abSnp": "cd android && ./gradlew clean && ./gradlew assembleAbivinSnpRelease && cd ..",
    "abivinDemoCPAndroid": "appcenter codepush release-react -a Abivin/Abivin-vRoute-Test-Android -d Staging -t 3.1.32 -m",
    "abivinDemoCPAndroidProduct": "appcenter codepush release-react -a Abivin/Abivin-vRoute-Test-Android -d Production -t 3.1.32 -m",
    "buildJs:ios": "react-native bundle --minify --entry-file index.js --platform ios --dev false --bundle-output ./ios/main.jsbundle --assets-dest ./ios",
    "buildJs:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res"
  },
  "dependencies": {
    "@mapbox/polyline": "0.2.0",
    "@mauron85/react-native-background-geolocation": "^0.6.3",
    "@react-native-community/async-storage": "1.11.0",
    "@react-native-community/cli-platform-android": "^4.13.0",
    "@react-native-community/netinfo": "4.1.5",
    "@react-native-firebase/analytics": "^7.6.9",
    "@react-native-firebase/app": "^8.4.7",
    "@react-native-firebase/auth": "^9.3.2",
    "@react-native-firebase/database": "^7.5.13",
    "@react-native-firebase/messaging": "^7.9.2",
    "@react-native-firebase/ml-vision": "^7.4.12",
    "@react-native-firebase/storage": "^7.4.11",
    "@types/react-native": "^0.63.32",
    "axios": "0.18.0",
    "lodash": "4.17.4",
    "lottie-ios": "2.5.0",
    "lottie-react-native": "2.5.11",
    "moment": "2.20.1",
    "prop-types": "15.5.10",
